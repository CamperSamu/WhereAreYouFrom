{
  "schemaVersion": 1,
  "id": "shoutout",
  "version": "${version}",

  "name": "Where Are You From?",
  "description": "A serverside Fabric mod that adds the corresponding mod name to items in the lore.",
  "authors": [
    "Camper_Samu"
  ],
  "contact": {
    "homepage": "https://github.com/CamperSamu/CamperSamu",
    "sources": "https://github.com/CamperSamu/WhereAreYouFrom"
  },

  "license": "CC0-1.0",
  "icon": "assets/shoutout/icon.png",

  "environment": "*",
  "entrypoints": {
    "server": [
      "com.campersamu.shoutout.Init"
    ]
  },
  "mixins": [
    "shoutout.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.0",
    "minecraft": "1.20.x",
    "java": ">=17"
  },
  "suggests": {
    "polydex": "*"
  }
}
